    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/react-big-calendar": "^1.8.9",
    "bcryptjs": "^3.0.2",
    "lucide-react": "^0.555.0",
    "moment": "^2.30.1",
    "next": "^15.5.9",
    "next-auth": "^4.24.11",
    "next-cloudinary": "^6.13.0",
    "pg": "^8.16.3",
    "react": "^18",
    "react-big-calendar": "^1.13.2",
    "react-calendar": "^5.0.0",
    "react-dom": "^18",
    "react-hook-form": "^7.52.2",
