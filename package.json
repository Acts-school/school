    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8",
    "eslint-config-next": "^15.2.4",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prisma": "^5.20.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vitest": "^4.0.16"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
